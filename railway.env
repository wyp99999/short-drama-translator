# Railway 环境变量配置
NODE_ENV=production
PORT=3001

# 数据库配置 - Railway 会自动注入
# DATABASE_URL=postgresql://...

# Redis 配置 - Railway 会自动注入
# REDIS_URL=redis://...

# 文件上传配置
MAX_FILE_SIZE=524288000
UPLOAD_DIR=/app/uploads
LOG_DIR=/app/logs
LOG_LEVEL=info

# 安全配置
JWT_SECRET=your-secret-key-change-in-production
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# 成本配置
COST_PER_MINUTE=10